{
    "name": "webhubworks/site-crawler",
    "description": "A straightforward site crawler",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://webhub.de",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/webhubworks/site-crawler/issues",
        "source": "https://github.com/webhubworks/site-crawler"
    },
    "authors": [
        {
            "name": "Johannes Ahrndt",
            "email": "support@webhub.de"
        }
    ],
    "require": {
        "php": "^8.4",
        "ext-dom": "*",
        "illuminate/http": "^v12.0.0",
        "illuminate/validation": "^12.0.0",
        "spatie/url": "^2.4"
    },
    "require-dev": {
        "laravel-zero/framework": "^12.0.0",
        "laravel/pint": "^1.15.2",
        "mockery/mockery": "^1.6.11",
        "pestphp/pest": "^v4.0.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/site-crawler"]
}
